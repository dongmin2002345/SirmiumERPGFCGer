<Controls:MetroWindow
        xmlns:Controls="http://metro.mahapps.com/winfx/xaml/controls"
        x:Class="SirmiumERPGFC.Views.OutputInvoices.OutputInvoice_ReportWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SirmiumERPGFC.Views.OutputInvoices"
         xmlns:rdlcreport="clr-namespace:Microsoft.Reporting.WinForms;assembly=Microsoft.ReportViewer.WinForms"
    mc:Ignorable="d"
        Title="{DynamicResource Izlazne_fakture}" Height="450" Width="800">
    <Grid>
        <GroupBox Grid.Row="0" Margin="2" Header="{DynamicResource Izazne_fakture}">
            <WindowsFormsHost Name="windowsFormsHost">
                <rdlcreport:ReportViewer x:Name="rdlcOutputInvoiceReport" />
            </WindowsFormsHost>
        </GroupBox>
    </Grid>
    </Controls:MetroWindow>
