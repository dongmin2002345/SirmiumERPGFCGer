<Window:MetroWindow
    xmlns:Window="http://metro.mahapps.com/winfx/xaml/controls" x:Class="SirmiumERPGFC.Views.ConstructionSites.ConstructionSite_ReportWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SirmiumERPGFC.Views.ConstructionSites"
        xmlns:rdlcreport="clr-namespace:Microsoft.Reporting.WinForms;assembly=Microsoft.ReportViewer.WinForms"
        mc:Ignorable="d"
        Title="{DynamicResource GRADILIŠTA}" Height="450" Width="800">
    <Grid>
        <GroupBox Grid.Row="0" Margin="2" Header="{DynamicResource GRADILIŠTA}">
            <WindowsFormsHost Name="windowsFormsHost">
                <rdlcreport:ReportViewer x:Name="rdlcConstructionSiteReport" />
            </WindowsFormsHost>
        </GroupBox>
    </Grid>
</Window:MetroWindow>
